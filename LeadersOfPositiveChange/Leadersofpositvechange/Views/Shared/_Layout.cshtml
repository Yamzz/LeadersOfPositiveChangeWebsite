<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>   </title>

    <!-- Facebook and Twitter integration -->
    <meta property="og:title" content="" />
    <meta property="og:image" content="" />
    <meta property="og:url" content="" />
    <meta property="og:site_name" content="" />
    <meta property="og:description" content="" />
    <meta name="twitter:title" content="" />
    <meta name="twitter:image" content="" />
    <meta name="twitter:url" content="" />
    <meta name="twitter:card" content="" />
    <!-- Icomoon Icon Fonts-->
    <link rel="stylesheet" href="~/Content/icomoon.css">
    <!-- Animate.css -->
    <link rel="stylesheet" href="~/Content/animate.css">

    <!-- Bootstrap  -->
    <link rel="stylesheet" href="~/Content/bootstrap.min.css">
    <!-- Superfish -->
    <link rel="stylesheet" href="~/Content/superfish.css">
    <link rel="stylesheet" href="~/Content/style.css">
    <link href="~/Content/images/others/new_icon.ico" rel="shortcut icon" type="image/x-icon" />

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


</head>
<body>

    <div id="fh5co-wrapper">
        <div id="fh5co-page">

            <div class="pagewide p-3 d-none d-lg-block fixed-top" style="border-bottom:solid 5px darkgreen; background-color:white">
                <div class="container">

                    <div class="col-lg-12" style="padding:5px;">
                        <div class="row">
                            <div class="col-lg-3 text-left" >
                                <img src="~/Content/images/others/new_logo.jpeg" width="100"/>
                            </div>

                            <div class="col-lg-6 text-center">
                                <a href="@Url.Action("LeadersOfPositiveChange", "Home")">
                                    <img id="logo-main" src="~/Content/images/others/new_leaders_logo.PNG" width="500" alt="Logo Thing main logo" style="margin-top:10px;margin-left:-50px;float:left">
                                </a>
                            </div>

                            <div class="col-lg-3 text-right">
                                <div style="margin: 30px auto; padding: 10px;position: center;">
                                    <a href="https://www.facebook.com/leadersofpositivecha/" target="_blank" class="grow"><i class="icon-facebook2"></i></a>
                                    <a href="https://www.instagram.com/leadersofpositive/" target="_blank" class="grow"><i class="icon-instagram"></i></a>
                                </div>
                            </div>
                        </div>

                    </div>

                    @*<div class="col-md-6 col-sm-6 text-right fh5co-link">
                        <a href="@Url.Action("Login", "Account")">Admin Login</a>
                    </div>*@
                </div>
            </div>

            <header id="fh5co-header-section" class="sticky-banner">
                <div class="container ">
                    <div class="nav-header">
                        <!-- START #fh5co-menu-wrap -->
                        <nav id="fh5co-menu-wrap" role="navigation" style="margin-bottom:10px">

                            <ul class="sf-menu" id="fh5co-primary-menu">
                                <li class="active">
                                    <a href="@Url.Action("LeadersOfPositiveChange", "Home")">Home</a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Volunteer", "Home")" class="fh5co-sub-ddown">Get Involved</a>
                                </li>
                                <li>
                                    <a href="@Url.Action("Gallery", "Home")" class="fh5co-sub-ddown">Our Gallery</a>
                                </li>
                                <li>
                                    <a href="#" class="fh5co-sub-ddown">About <i class="icon-arrow-down22"></i></a>

                                    <ul class="fh5co-sub-menu">
                                        <li>@Html.ActionLink("About Us", "AboutUs", "Home")</li>
                                        <li>@Html.ActionLink("Our Team", "OurTeam", "Home")</li>
                                    </ul>
                                </li>

                                <li>@Html.ActionLink("Our Blog", "OurBlog", "Home")</li>
                                <li>@Html.ActionLink("Contact", "ContactUs", "Home")</li>
                                <li id="donateBtn"> @Html.ActionLink("Donations", "Donate", "Donation")</li>

                            </ul>
                        </nav>
                    </div>
                </div>
            </header>


            <div class="container-fluid body-content">
                @RenderBody()
            </div>

            <!-- Footer -->
            <!-- Footer -->
            <footer>
                <div id="footer">         
                    <div class="container">
                        <div class="row">
                            <div class="col-md-6 col-md-offset-3 text-center">
                                <p class="fh5co-social-icons">
                                    <a href="https://www.facebook.com/leadersofpositivecha/" target="_blank"><i class="icon-facebook2"></i></a>
                                    <a href="https://www.instagram.com/leadersofpositive/" target="_blank"><i class="icon-instagram"></i></a>
                                </p>
                                <br />
                                <p>© Copyright at <strong>  Leaders Of Positive Change </strong>  All Rights Reserved.</p>
                                <p>Developed by <strong>  BIOCONICSOL LTD</strong></p>
                            </div>
                        </div>
                    </div>
                </div>
            </footer>


        </div>
        <!-- END fh5co-page -->
    </div>
    <!-- END fh5co-wrapper -->


    <script src="~/Scripts/jquery.min.js"></script>
    @*<script src="~/Scripts/jquery-3.3.1.min.js"></script>*@
    <script src="~/Scripts/superfish.js"></script>

    <script src="~/Scripts/jquery.easing.1.3.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.waypoints.min.js"></script>
    <script src="~/Scripts/sticky.js"></script>
    <script src="~/Scripts/jquery.stellar.min.js"></script>
    <script src="~/Scripts/hoverIntent.js"></script>

    <script src="~/Scripts/main.js"></script>

    <script src="~/Scripts/modernizr-2.6.2.min.js"></script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    @RenderSection("styles", required: false)
</body>
</html>
